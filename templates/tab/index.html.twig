{% extends 'template.html.twig' %}

{% block body %}
<table>
    {% for note in notes %}
    <tr>
        <td>{{note}}</td>
    
    </tr>
    {% endfor %}


</table>
<div class="alert alert-info">
    <p>Nombre de notes : {{notes | length}}</p>
    <p>Nombre max : {{max(notes)}}</p>
    <p>Nombre min : {{min(notes)}}</p>

</div>
{% endblock %}
